<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title>Interact JS</title>
  </head>
  <body>
    <ul>
      <% interact.forEach(each => { %>
        <li>
          <a href="/interact/<%= each.route %>">
            <%= each.title %>
          </a>
        </li>
      <% }) %>
    </ul>
  </body>
</html>
