          <div class="item_menu__interface">
            <div class="item_menu__interface__variant text" data-mos-type="text">
              <div class="insert new_text__insert">
                <div class="insert__title">
                  <h3>INSERT TEXT BOX</h3>
                </div>
                <ul class="new_text__size">
                  <li>
                    <button type="button" name="small" class="new_text__size--small">A</button>
                  </li>
                  <li>
                    <button type="button" name="medium" class="new_text__size--medium active">A</button>
                  </li>
                  <li>
                    <button type="button" name="large" class="new_text__size--large">A</button>
                  </li>
                </ul>
                <div class="new_text__value">
                  <textarea name="name" rows="4" placeholder="Text box content"></textarea>
                </div>
                <div class="insert__action_butons">
                  <button type="button" name="new_text__insert">Insert Text Box +</button>
                </div>
              </div>
            </div>
            <div class="item_menu__interface__variant image" data-mos-type="image">
              <div class="insert new_image__insert">
                <div class="insert__title">
                  <h3>INSERT IMAGE</h3>
                </div>
                <div class="image_interface--wrapper">
                  <label for="image_upload">Upload Image</label>
                  <input type="file" name="image_upload" class="image_upload" id="image_upload" />
                  <input type="url" name="image_url" class="image_url" placeholder="Paste Image URL" />
                  <div class="image_interface__preview">
                    <img src="https://res.cloudinary.com/oddert/image/upload/v1558101908/MOS/tiles/placeholder_image-01.png" alt="Placeholder Image">
                  </div>
                </div>
                <div class="insert__action_butons">
                  <button type="button" name="new_image__insert">Insert Image +</button>
                </div>
              </div>
            </div>
            <div class="item_menu__interface__variant product active" data-mos-type="product">

              <div class="insert new_product__insert">
                <div class="insert__title">
                  <h3>INSERT PRODUCT</h3>
                </div>
                <div class="product_search__input">
                  <div class="product_search__input--wrapper">
                    <input class="product_search__input--bar" type="text" name="product_search" placeholder="Search MOS Products..." />
                    <button class="product_search__input--clear" type="button" name="product_search_clear">
                      <i class="fas fa-times"></i>
                    </button>
                  </div>
                </div>
                <div class="product_search__results">
                  <ul>

                  </ul>
                </div>
                <div class="insert__action_butons">
                  <button type="button" name="new_product__insert">Insert Product +</button>
                </div>
              </div>

            </div>
            <div class="item_menu__interface__variant material" data-mos-type="material">

