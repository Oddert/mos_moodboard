                    <input class="product_search__input--bar" type="text" name="product_search" placeholder="Search the Material Library..." />
                    <button class="product_search__input--clear" type="button" name="product_search_clear">
                      <i class="fas fa-times"></i>
                    </button>
                  </div>
                </div>
                <div class="product_search__results">
                  <ul>

                  </ul>
                </div>
                <div class="insert__action_butons">
                  <button type="button" name="new_material__insert">Insert Material +</button>
                </div>
              </div>

            </div>
