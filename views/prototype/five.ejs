<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title></title>
    <script src="https://cdn.jsdelivr.net/npm/@jaames/iro/dist/iro.min.js"></script>
    <!-- NOTE: Temporary font awesome using my own auth token -->
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.4.1/css/all.css" integrity="sha384-5sAR7xN1Nv6T6+dT2mhtzEpVJvfS3NScPQTrOxhwjIuvcA67KV2R5Jz6kr4abQsz" crossorigin="anonymous">

    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/gridstack.js/0.4.0/gridstack.min.css" />
    <!-- WARNING: older version of jQuery, current is 3.4.1 -->
    <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.12.1/jquery-ui.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/lodash.js/4.17.15/lodash.min.js"></script>
    <script type="text/javascript" src='//cdnjs.cloudflare.com/ajax/libs/gridstack.js/0.4.0/gridstack.min.js'></script>
    <script type="text/javascript" src='//cdnjs.cloudflare.com/ajax/libs/gridstack.js/0.4.0/gridstack.jQueryUI.min.js'></script>

    <link rel="stylesheet" href="/css/master.css">
    <link rel="stylesheet" href="/css/gridstack_extra.css">
  </head>
  <body>
    <!-- <button name="dev">Re Render</button> -->
    <div class="pages--container menu_open">
      <div class="pages">

      </div>
    </div>
    <div class="new_item_menu--container">
      <div class="new_item_menu">
        <ul>
          <li class="new_item_menu__item text" title="Create Text Box">
            <button type="button" name="New Text">
              T
            </button>
          </li>
          <li class="new_item_menu__item image" title="Insert Image">
            <button type="button" name="New Image">
              <i class="fas fa-image"></i>
            </button>
          </li>
          <li class="new_item_menu__item product" title="Add Product">
            <button type="button" name="New Product">
              <i class="fas fa-chair"></i>
            </button>
          </li>
          <li class="new_item_menu__item material" title="Add Material">
            <button type="button" name="New Material">
              <i class="fas fa-dice-d6"></i>
              <!-- <i class="fas fa-dice-d20"></i> -->
              <!-- <i class="fas fa-swatchbook"></i> -->
            </button>
          </li>
          <li class="new_item_menu__item colour" title="New Colour Swatch">
            <button type="button" name="New Colour">
              <i class="fas fa-palette"></i>
            </button>
          </li>
          <li class="new_item_menu__item file" title="Insert File">
            <button type="button" name="New File">
              <i class="fas fa-file-alt"></i>
            </button>
          </li>
        </ul>
      </div>
    </div>
    <div class="colour_picker">
      <div class="colour_picker__iro">

      </div>
      <div class="colour_picker__preview">
        <div class="colour_picker__preview--wrapper">
          <input value="#1bbc9b" class="colour_picker__preview__input" />
        </div>
      </div>
      <div class="colour_picker__control">
        <div class="colour_picker__control--wrapper">
          <button type="button" name="colour_cancel" class="colour_cancel">
            <i class="fas fa-times"></i>
          </button>
          <button type="button" name="colour_accept" class="colour_accept">
            <i class="fas fa-check"></i>
          </button>
        </div>
      </div>
    </div>
    <div class="dashboard--container">

    </div>
    <ul class="control_display">
      <li>
        <button name="scroll" class="control_display--scroll active">Scroll</button>
      </li>
      <li>
        <button name="fullscreen" class="control_display--fullscreen">Full Screen</button>
      </li>
      <li>
        <button name="slides" class="control_display--slides">Slides</button>
      </li>
    </ul>
  </body>
  <link rel="stylesheet" href="/css/prototype/five.css">
  <script type="text/javascript" src="/js/prototype/five.js"></script>
</html>
